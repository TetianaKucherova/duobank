<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke Tests Suite" parallel="tests" thread-count="10">



    <test name="Smoke Tests UI">

        <groups>

            <run>
                <include name="smoke_test"></include>
            </run>

        </groups>


        <packages>
            <package name=".*"></package>
        </packages>

    </test> <!-- Test -->
</suite> <!-- Suite -->